/*modal*/

.people_alumni_modal_class {
    position: fixed; /* Stay in place */
    z-index: 1; /* Sit on top */
    padding-top: 100px; /* Location of the box */
    left: 0;
    top: 0;
    width: 100%; /* Full width */
    height: 100%; /* Full height */
    overflow: auto; /* Enable scroll if needed */
    background-color: rgb(0,0,0); /* Fallback color */
    background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
}

.people_alumni_modal_content {
    background-color: #fefefe;
    margin: auto;
    padding: 20px;
    border: 1px solid #888;
    width: 50%;
}

.people_alumni_modal_close{
    color: #aaaaaa;
    float: right;
    font-size: 28px;
    font-weight: bold;
}

.people_alumni_modal_close:hover,
.people_alumni_modal_close:focus {
    color: #000;
    text-decoration: none;
    cursor: pointer;
}

.people_alumni_modal_class .people_story_hidden{
    display: none;
}

.people_alumni_modal_class .people_story_more{
    cursor: pointer;
    font-weight: bold;
    display: inline-block;
}









