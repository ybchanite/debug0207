/*initialize the font family*/


body{
    font-family: "mpr", "mps", "mpb" , "Helvetica";

}

/*ie*/
@font-face {
    font-family: "mps";
    src: url('./MPS.woff?#iefix') format('embedded-opentype');
}


@font-face {
    font-family: "mps";
    src: url(MPS.otf) format("truetype");
}


/*ie*/
@font-face {
    font-family: "mpr";
    
    src: url('./MPR.eot?#iefix') format('embedded-opentype');
}

@font-face {
    font-family: "mpr";
    
    src: url('./MPR.eot');
    src: url(MPR.otf) format("truetype");
}


/*ie*/
@font-face {
    font-family: "mpb";
    src: url('./MPB.woff?#iefix') format('embedded-opentype');
}
@font-face {
    font-family: "mpb";
    src: url('./MPB.eot');    
    src: url(MPB.otf) format("truetype");
}


